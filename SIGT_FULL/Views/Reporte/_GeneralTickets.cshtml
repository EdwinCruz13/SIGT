<h4>
    <small class="pull-left" style="color:#9e9e9e"><i class="@ViewBag.Icon"></i> <b>@ViewBag.Text</b></small>
    <span class="clearfix"></span>
</h4>

<div class="col-md-4">
    <div class="row">
        <h3 style="color:#1976d2"><b>@string.Format("{0:n}", ViewBag.Generados).Replace(".00", "") <small>tickets</small></b></h3>
    </div>

    <div class="row">
        <h4 style="margin-top:-0.3rem"><b><small>Generados</small></b></h4>
    </div>
</div>

<div class="col-md-4">
    <div class="row">
        <h3 style="color:#1976d2"><b>@string.Format("{0:n}", ViewBag.Atendidos).Replace(".00", "")</b><small> tickets</small></h3>
    </div>

    <div class="row">
        <h4 style="margin-top:-0.3rem"><b><small>Atendidos <span style="color:#712525; font-size:12px"><i class="glyphicon glyphicon-arrow-down"></i> @string.Format("{0:n}", ViewBag.Anulados) % An.</span></small></b></h4>
    </div>
</div>

<div class="col-md-4" style="padding-left:2rem">
    <div class="row">
        <h3 style="color:#1976d2"><b>@string.Format("{0:n}", ViewBag.Tiempo) <small>Horas</small></b></h3>
    </div>

    <div class="row">
        <h4 style="margin-top:-0.3rem"><b><small>Tiempo en atención</small></b></h4>
    </div>
</div>
